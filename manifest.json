{
  "name": "dono$",
  "description": "Расширение содержащие в себе базу жалоб пользователей вк",
  "version": "1.0",
  "homepage_url": "https://github.com/Paradisse/vkdonos",
  "manifest_version": 2,
  "permissions": [
    "storage",
    "activeTab",
    "https://vk.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://vk.com/*"],
      "js": [
        "js/main.js",
        "js/rating.js",
        "js/black.js",
        "js/new_loader.js"
      ],
      "css": [
        "css/styles.css",
        "css/test.css",
        "css/black.css",
        "css/new_loader.css"
      ],
      "run_at": "document_end"
    }
  ]
}